local MinimzerButton = Instance.new("TextButton")

MinimizerReapah.Name = "MinimizerReapah"
MinimizerReapah.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
MinimizerReapah.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MinimzerButton.Name = "MinimzerButton"
MinimzerButton.Parent = MinimizerReapah
MinimzerButton.BackgroundColor3 = Color3.fromRGB(79, 79, 79)
MinimzerButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
MinimzerButton.BorderSizePixel = 0
MinimzerButton.Position = UDim2.new(0.458159536, 0, 0.0849673226, 0)
MinimzerButton.Size = UDim2.new(0, 48, 0, 21)
MinimzerButton.Font = Enum.Font.SourceSans
MinimzerButton.Text = "Minimize"
MinimzerButton.TextColor3 = Color3.fromRGB(0, 0, 0)
MinimzerButton.TextScaled = true
MinimzerButton.TextSize = 14.000
MinimzerButton.TextWrapped = true

MinimzerButton.MouseButton1Down:connect(function()
	if game.Players.LocalPlayer.PlayerGui.DAGRIMREAPAH.Enabled == true then
		game.Players.LocalPlayer.PlayerGui.DAGRIMREAPAH.Enabled = false
	else
		game.Players.LocalPlayer.PlayerGui.DAGRIMREAPAH.Enabled = true
	end
end)


